    "nameOriginal": "npmdoc-debug-logger",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-debug-logger.git"
    },
    "scripts": {
